{
    "swagger": "2.0",
    "info": {
        "description": "This is a sample server for xinde.",
        "title": "xinde API",
        "termsOfService": "http://swagger.io/terms/",
        "contact": {
            "name": "API Support",
            "url": "http://www.swagger.io/support",
            "email": "support@swagger.io"
        },
        "license": {
            "name": "Apache 2.0",
            "url": "http://www.apache.org/licenses/LICENSE-2.0.html"
        },
        "version": "1.0"
    },
    "host": "localhost:8080",
    "basePath": "/api/v1",
    "paths": {
        "/api/v1/account/login": {
            "post": {
                "description": "用户登录，返回JWT token和用户基本信息",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "Account"
                ],
                "summary": "用户登录",
                "parameters": [
                    {
                        "description": "Login Request",
                        "name": "request",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/xinde_internal_dto_account.LoginReq"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "登录成功",
                        "schema": {
                            "$ref": "#/definitions/xinde_internal_dto_account.LoginResp"
                        }
                    },
                    "400": {
                        "description": "参数错误",
                        "schema": {
                            "$ref": "#/definitions/xinde_pkg_response.Response"
                        }
                    },
                    "401": {
                        "description": "用户名或密码错误",
                        "schema": {
                            "$ref": "#/definitions/xinde_pkg_response.Response"
                        }
                    },
                    "500": {
                        "description": "服务器内部错误",
                        "schema": {
                            "$ref": "#/definitions/xinde_pkg_response.Response"
                        }
                    }
                }
            }
        },
        "/api/v1/account/register": {
            "post": {
                "description": "用户名，真实姓名，公司名称，公司地址（可选），密码，手机号，邮箱（可选）",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "Account"
                ],
                "summary": "注册一个新用户",
                "parameters": [
                    {
                        "description": "Register Request",
                        "name": "request",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/xinde_internal_dto_account.RegisterReq"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "User registered successfully",
                        "schema": {
                            "$ref": "#/definitions/xinde_internal_dto_account.RegisterResp"
                        }
                    },
                    "400": {
                        "description": "Bad Request",
                        "schema": {
                            "$ref": "#/definitions/xinde_internal_dto_account.RegisterResp"
                        }
                    },
                    "403": {
                        "description": "Forbidden",
                        "schema": {
                            "$ref": "#/definitions/xinde_internal_dto_account.RegisterResp"
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "$ref": "#/definitions/xinde_internal_dto_account.RegisterResp"
                        }
                    }
                }
            }
        },
        "/api/v1/admin/account/list": {
            "get": {
                "description": "返回已被通过注册申请的用户信息",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "Account"
                ],
                "summary": "管理员查看用户列表",
                "parameters": [
                    {
                        "description": "Login Request",
                        "name": "request",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/xinde_internal_dto_account.ListReq"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "登录成功",
                        "schema": {
                            "$ref": "#/definitions/xinde_internal_dto_account.ListResp"
                        }
                    },
                    "400": {
                        "description": "参数错误",
                        "schema": {
                            "$ref": "#/definitions/xinde_pkg_response.Response"
                        }
                    },
                    "500": {
                        "description": "服务器内部错误",
                        "schema": {
                            "$ref": "#/definitions/xinde_pkg_response.Response"
                        }
                    }
                }
            }
        },
        "/ping": {
            "get": {
                "description": "do ping",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "health"
                ],
                "summary": "Ping server",
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "object",
                            "additionalProperties": true
                        }
                    }
                }
            }
        }
    },
    "definitions": {
        "xinde_internal_dto_account.ListData": {
            "type": "object",
            "properties": {
                "company_name": {
                    "type": "string",
                    "example": "宁波鲍斯产业链服务有限公司"
                },
                "created_at": {
                    "type": "string",
                    "example": "2020-09-08 09:08:09"
                },
                "email": {
                    "type": "string",
                    "example": "13800138000@qq.com"
                },
                "id": {
                    "type": "integer",
                    "example": 2
                },
                "name": {
                    "type": "string",
                    "example": "张三，真实名称"
                },
                "phone": {
                    "type": "string",
                    "example": "13800138000"
                },
                "price_level": {
                    "type": "string",
                    "example": "price_1"
                },
                "recent_search_at": {
                    "type": "string",
                    "example": "2020-09-08 09:08:09"
                },
                "remark": {
                    "type": "string",
                    "example": "备注"
                },
                "role": {
                    "type": "string",
                    "example": "普通用户"
                },
                "search_device": {
                    "type": "string",
                    "example": "车削刀杆"
                },
                "username": {
                    "type": "string",
                    "example": "张三，账号名称"
                }
            }
        },
        "xinde_internal_dto_account.ListPageData": {
            "type": "object",
            "properties": {
                "list": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/xinde_internal_dto_account.ListData"
                    }
                },
                "page": {
                    "type": "integer",
                    "example": 1
                },
                "pageSize": {
                    "type": "integer",
                    "example": 20
                },
                "pages": {
                    "type": "integer",
                    "example": 7
                },
                "total": {
                    "type": "integer",
                    "example": 137
                }
            }
        },
        "xinde_internal_dto_account.ListReq": {
            "type": "object"
        },
        "xinde_internal_dto_account.ListResp": {
            "type": "object",
            "properties": {
                "code": {
                    "type": "integer",
                    "example": 200
                },
                "data": {
                    "$ref": "#/definitions/xinde_internal_dto_account.ListPageData"
                },
                "message": {
                    "type": "string",
                    "example": "操作成功"
                },
                "success": {
                    "type": "boolean",
                    "example": true
                }
            }
        },
        "xinde_internal_dto_account.LoginData": {
            "type": "object",
            "properties": {
                "access_token": {
                    "type": "string",
                    "example": "jwt_token"
                },
                "email": {
                    "type": "string",
                    "example": "1921771473@qq.com"
                },
                "name": {
                    "type": "string",
                    "example": "金晖"
                },
                "phone": {
                    "type": "string",
                    "example": "13065859690"
                },
                "username": {
                    "type": "string",
                    "example": "金晖"
                }
            }
        },
        "xinde_internal_dto_account.LoginReq": {
            "type": "object",
            "required": [
                "password",
                "username"
            ],
            "properties": {
                "password": {
                    "type": "string",
                    "example": "923845797582"
                },
                "username": {
                    "type": "string",
                    "example": "金晖"
                }
            }
        },
        "xinde_internal_dto_account.LoginResp": {
            "type": "object",
            "properties": {
                "code": {
                    "type": "integer",
                    "example": 200
                },
                "data": {
                    "$ref": "#/definitions/xinde_internal_dto_account.LoginData"
                },
                "message": {
                    "type": "string",
                    "example": "操作成功"
                },
                "success": {
                    "type": "boolean",
                    "example": true
                }
            }
        },
        "xinde_internal_dto_account.RegisterReq": {
            "type": "object",
            "required": [
                "company_name",
                "confirmed_password",
                "name",
                "password",
                "phone",
                "username"
            ],
            "properties": {
                "company_address": {
                    "type": "string",
                    "example": "浙江省宁波市奉化区江口街道聚潮路55号，可选"
                },
                "company_name": {
                    "type": "string",
                    "example": "宁波鲍斯产业链服务有限公司"
                },
                "confirmed_password": {
                    "type": "string",
                    "example": "923845797582"
                },
                "email": {
                    "type": "string",
                    "example": "1921771473@qq.com，可选"
                },
                "name": {
                    "type": "string",
                    "example": "金晖，真实姓名"
                },
                "password": {
                    "type": "string",
                    "example": "923845797582"
                },
                "phone": {
                    "type": "string",
                    "example": "13065859690"
                },
                "username": {
                    "type": "string",
                    "example": "金晖，账号名称"
                }
            }
        },
        "xinde_internal_dto_account.RegisterResp": {
            "type": "object",
            "properties": {
                "code": {
                    "type": "integer",
                    "example": 200
                },
                "data": {},
                "message": {
                    "type": "string",
                    "example": "操作成功"
                },
                "success": {
                    "type": "boolean",
                    "example": true
                }
            }
        },
        "xinde_pkg_response.Response": {
            "type": "object",
            "properties": {
                "code": {
                    "description": "业务状态码",
                    "type": "integer"
                },
                "data": {
                    "description": "响应数据，空时不返回该字段"
                },
                "message": {
                    "description": "响应消息",
                    "type": "string"
                },
                "success": {
                    "description": "是否成功",
                    "type": "boolean"
                }
            }
        }
    }
}